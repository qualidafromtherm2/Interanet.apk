{
  "expo": {
    "name": "Fromtherm",
    "slug": "loginapp",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

    "icon": "./assets/fromtherm-icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.qualidafromtherm2.loginapp",
      "versionCode": 2,
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/fromtherm-adaptive-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.20"
          }
        }   
      ],
      "expo-secure-store",
      "expo-font"
    ],

    "platforms": ["ios", "android", "web"],
    "sdkVersion": "54.0.0",
    "extra": {
      "eas": {
        "projectId": "a008e885-d5be-469d-8d8c-2e0bd4745063"
      }
    }
  }
}
